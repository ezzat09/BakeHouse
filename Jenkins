pipeline { 
    agent any 
    stages {
          stage('EKS start'){
          steps{
            sh "aws eks --region us-east-1 update-kubeconfig --name lab1"
            }
          }
          stage('deployment'){
           steps{
            sh "kubectl create -f deployment.yaml"
           }
         }
        stage('loadbalancer'){
           steps{
            sh "kubectl create -f service.yaml"
           }
         }
        }  
      }
